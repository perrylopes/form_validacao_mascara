        text-transform: uppercase
        width: 100%
        height: 100%
        display flex
        align-items: center
        justify-content: space-around
        color: cinza-linha
        text-decoration: none

#form_m
    border-right: 1px solid cinza-linha
    background-color: cinza-fundo

#form_f
    background-color: cinza-fundo
    

.form_anexo
    display none
    + label
        color #ffffff
        height: 40px
        width: 300px
        border 0
        margin-left: 50px
        display: flex
        align-items: center
        justify-content: center
        

